! Xterm
XTerm*background:       Black
XTerm*foreground:       Green
XTerm*bellSuppressTime: 250
XTerm*boldColors:       true
XTerm*boldMode:         false
XTerm*cacheDoublesize:  4
XTerm*eightBitInput:    false
XTerm*metaSendsEscape:  true
XTerm*utf8:             1
XTerm*saveLines:        5000
XTerm*trimSelection:    true
XTerm*underLine:        true
XTerm*jumpScroll:       true
XTerm*scrollBar:        false
XTerm*scrollBar_right:  true
XTerm*dynamicColors:    true
XTerm*bellIsUrgent:     true
XTerm*scrollKey:        true
XTerm*scrollTtyOutput:  false

! Urxvt
URxvt*background:       Black
URxvt*foreground:       White
URxvt.font: xft: MesloLGS Nerd Font Mono:size=15

URxvt*jumpScroll: true
URxvt*urgentOnBell: true
URxvt*scrollBar: false
URxvt*scrollBar_right:  true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: false
URxvt*scrollTtyKeypress: true

*termName: xterm-256color
*mouseWheelScrollPage: false
*cursorBlink: true
*cursorColor: #EA2B03

URxvt.perl-ext-common : default,matcher
URxvt.urlLauncher     : /usr/bin/brave-browser
URxvt.matcher.button  : 1 

URxvt.iso14755: true
URxvt.iso14755_52: true

! black
*.color0: #263640
*.color8: #4a697d

! red
*.color1: #d12f2c
*.color9: #fa3935

! green
*.color2: #819400
*.color10: #a4bd00

! yellow
*.color3: #b08500
*.color11: #d9a400

! blue
*.color4: #2587cc
*.color12: #2ca2f5

! magenta
*.color5: #696ebf
*.color13: #8086e8

! cyan
*.color6: #289c93
*.color14: #33c5ba

! white
*.color7: #bfbaac
*.color15: #fdf6e3
